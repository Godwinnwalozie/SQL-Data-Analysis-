 /* CREATE TABLE */
CREATE TABLE IF NOT EXISTS revdata(
CampaignID varchar,
Timestamp VARCHAR( 100 ),
Date VARCHAR( 100 ),
InventoryType VARCHAR( 100 ),
AdType VARCHAR( 100 ),
Available_Impressions VARCHAR( 100 ),
Matched_Queries VARCHAR( 100 ),
Impressions VARCHAR( 100 ),
Clicks VARCHAR( 100 ),
Spend VARCHAR( 100 ),
Fee VARCHAR( 100 ),
Revenue VARCHAR( 100 ),
CTR VARCHAR( 100 ),
CPM VARCHAR( 100 ),
CPC VARCHAR( 100 )
);

drop table if exists revdata cascade

